# Configuração de ambiente para Google AI for Developers
# Corrigido para usar modelos válidos sem sufixo -002

# Google AI for Developers API Key
GOOGLE_GENERATIVE_AI_API_KEY=your-google-api-key-here
# Alternativas (se necessário)
GOOGLE_API_KEY=your-google-api-key-here
GEMINI_API_KEY=your-google-api-key-here
GOOGLE_GEMINI_API_KEY=your-google-api-key-here

# Modelos Google recomendados (2025)
# Modelos válidos para Google AI for Developers (sem sufixo -002):
GOOGLE_MODEL_DEFAULT=gemini-2.5-flash
GOOGLE_MODEL_CLASSIFY=gemini-2.5-flash
GOOGLE_MODEL_CHAT=gemini-2.5-pro
GOOGLE_MODEL_COMPLEX=gemini-2.5-pro
GOOGLE_MODEL_FAST=gemini-2.5-flash

# Configurações de fallback
AI_PROVIDER=google
FALLBACK_PROVIDER=openai

# OpenAI (para fallback)
OPENAI_API_KEY=your-openai-api-key-here

# Configurações de cache e performance
ENABLE_MODEL_CACHE=true
CACHE_TTL_MINUTES=60
MAX_CACHE_SIZE=1000

# Logs de debug (desabilitar em produção)
DEBUG_MODEL_SELECTION=false
LOG_MODEL_CALLS=false
