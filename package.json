{
  "name": "hubedu-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "health": "curl -f http://localhost:${PORT:-10000}/api/health || exit 1",
    "healthz": "curl -f http://localhost:${PORT:-10000}/api/healthz || exit 1",
    "health:enem": "curl -f http://localhost:${PORT:-10000}/api/enem/health || exit 1",
    "test:health": "npm run health && npm run healthz && npm run health:enem",
    "health:full": "./scripts/health-check.sh",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test:auth": "node scripts/test-auth.js",
    "create:test-user": "node scripts/create-test-user.js",
    "setup": "chmod +x scripts/*.sh && ./scripts/setup-dev.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "node tests/performance/load-test.js",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:prompts": "jest --testPathPattern=prompts",
    "test:api": "jest --testPathPattern=api",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "migrate:from-hubedu": "node scripts/migrate-from-hubedu.js",
    "migrate:finalize": "node scripts/finalize-migration.js",
    "migrate:complete": "npm run migrate:from-hubedu && npm run migrate:finalize"
  },
