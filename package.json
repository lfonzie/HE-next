{
  "name": "hubedu-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others --prefix-colors \"blue,green\" --names \"HubEdu,ENEM-API\" \"next dev\" \"cd enem-api-main && npm run dev\"",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:turbo": "next dev --turbo",
    "dev:all": "chmod +x scripts/*.sh && ./scripts/start-all.sh",
    "build": "npm run build:hubedu && npm run build:enem",
    "build:hubedu": "next build",
    "build:enem": "cd enem-api-main && npm ci && npx prisma generate && npm run build",
    "start": "concurrently --kill-others --prefix-colors \"blue,green\" --names \"HubEdu,ENEM-API\" \"next start\" \"cd enem-api-main && npm start\"",
    "start:hubedu": "next start",
    "start:enem": "cd enem-api-main && npm start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test:auth": "node scripts/test-auth.js",
    "create:test-user": "node scripts/create-test-user.js",
    "setup": "chmod +x scripts/*.sh && ./scripts/setup-dev.sh"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@hookform/resolvers": "^3.0.0",
    "@monaco-editor/react": "^4.7.0",
    "@prisma/client": "^6.16.1",
    "@radix-ui/react-accordion": "^1.1.0",
    "@radix-ui/react-alert-dialog": "^1.0.0",
    "@radix-ui/react-avatar": "^1.0.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-progress": "^1.0.0",
    "@radix-ui/react-scroll-area": "^1.0.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.0.0",
    "@radix-ui/react-toast": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.400.0",
    "nanoid": "^5.0.0",
    "next": "^15.5.3",
    "next-auth": "^4.24.0",
    "openai": "^4.0.0",
    "prisma": "^6.16.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.0.0",
    "react-markdown": "^9.0.0",
    "rehype-highlight": "^7.0.0",
    "rehype-katex": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "sonner": "^1.0.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.0.0",
    "y18n": "^5.0.8",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
