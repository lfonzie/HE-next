{
  "name": "hubedu-nextjs",
  "version": "0.0.42",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:turbo:fix": "./scripts/fix-turbopack-permissions.sh && npm run dev:turbo",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:turbo-alt": "./dev-turbo-alternative.sh",
    "build": "next build",
    "build:render": "npm ci --include=dev && npm run build",
    "build:turbo": "next build --turbopack",
    "start": "next start -p $PORT",
    "health": "curl -f http://localhost:${PORT:-10000}/api/health || exit 1",
    "healthz": "curl -f http://localhost:${PORT:-10000}/api/healthz || exit 1",
    "health:enem": "curl -f http://localhost:${PORT:-10000}/api/enem/health || exit 1",
    "test:health": "npm run health && npm run healthz && npm run health:enem",
    "health:full": "./scripts/health-check.sh",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test:auth": "node scripts/test-auth.js",
    "create:test-user": "node scripts/create-test-user.js",
    "setup": "chmod +x scripts/*.sh && ./scripts/setup-dev.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:regression": "node scripts/run-regression-tests.js",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:prompts": "jest --testPathPattern=prompts",
    "test:api": "jest --testPathPattern=api",
    "test:ai-sdk": "node test-ai-sdk-integration.js",
    "test:perplexity": "node test-perplexity-integration.js",
    "test:multi-provider": "node test-multi-provider.js",
    "test:smart-router": "node test-smart-router.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "otel:start": "docker-compose -f docker-compose.otel.yml up -d",
    "otel:stop": "docker-compose -f docker-compose.otel.yml down",
    "otel:logs": "docker-compose -f docker-compose.otel.yml logs -f",
    "otel:status": "docker-compose -f docker-compose.otel.yml ps",
    "migrate:from-hubedu": "node scripts/migrate-from-hubedu.js",
    "migrate:finalize": "node scripts/finalize-migration.js",
    "migrate:complete": "npm run migrate:from-hubedu && npm run migrate:finalize",
    "benchmark": "node scripts/run-benchmark.js",
    "benchmark:quick": "node scripts/benchmark-quick.js",
    "benchmark:aulas": "node scripts/benchmark-aulas-final.js",
    "benchmark:models": "node scripts/benchmark-real-models.js",
    "benchmark:multi": "node scripts/benchmark-models-comparison.js",
    "cleanup:enem": "node scripts/cleanup-enem-invalid-questions.js"
  },
