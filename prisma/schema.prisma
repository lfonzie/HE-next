model paragraph_feedback {
  id           String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  session_id   String   @db.Uuid
  paragraph_id String   @db.Uuid
  idx          Int
  comp1        Int?
  comp2        Int?
  comp3        Int?
  comp4        Int?
  comp5        Int?
  suggestions  Json?
  summary_note String?
  ai_model     String?  @db.VarChar(50)
  created_at   DateTime @default(now()) @db.Timestamp(6)
}

model quiz_attempts {
  id         String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user_id    String   @db.Uuid
  lesson_id  String   @db.VarChar(50)
  item_id    String   @db.VarChar(50)
  choice     Int
  correct    Boolean
  confidence Int?
  ms_taken   Int?
  tags       Json?
  created_at DateTime @default(now()) @db.Timestamp(6)
}

model quiz_items {
  id            String   @id @db.VarChar(50)
  stem          String
  choices       Json
  correct_index Int
  rationale     String?
  tags          Json?
  difficulty    String?  @default("medium") @db.VarChar(20)
  lesson_id     String?  @db.VarChar(50)
  user_id       String?  @db.Uuid
  created_at    DateTime @default(now()) @db.Timestamp(6)
  updated_at    DateTime @default(now()) @db.Timestamp(6)
}

model quotas {
  id          String            @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user_id     String            @db.Uuid
  month       String            @db.VarChar(7)
  token_limit Int               @default(100000)
  token_used  Int               @default(0)
  created_at  DateTime          @default(now()) @db.Timestamp(6)
  updated_at  DateTime?         @default(now()) @db.Timestamp(6)
  is_active   Boolean           @default(true)
  usage_logs  quota_usage_log[]
  user        User              @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@unique([user_id, month], map: "unique_user_month_quota")
  @@index([user_id], map: "idx_quotas_user")
  @@index([month], map: "idx_quotas_month")
}

model quota_usage_log {
  id                String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  quota_id          String   @db.Uuid
  user_id           String   @db.Uuid
  provider          String   @db.VarChar(50)
  model             String   @db.VarChar(100)
  prompt_tokens     Int      @default(0)
  completion_tokens Int      @default(0)
  total_tokens      Int      @default(0)
  cost_usd          Decimal  @default(0) @db.Decimal(10, 6)
  cost_brl          Decimal  @default(0) @db.Decimal(10, 6)
  module            String?  @db.VarChar(50)
  conversation_id   String?  @db.Uuid
  api_endpoint      String?  @db.VarChar(100)
  success           Boolean  @default(true)
  error_message     String?
  created_at        DateTime @default(now()) @db.Timestamp(6)
  quota             quotas   @relation(fields: [quota_id], references: [id], onDelete: Cascade)
  user              User     @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@index([quota_id], map: "idx_quota_usage_log_quota")
  @@index([user_id], map: "idx_quota_usage_log_user")
  @@index([created_at], map: "idx_quota_usage_log_created")
  @@index([provider], map: "idx_quota_usage_log_provider")
  @@index([model], map: "idx_quota_usage_log_model")
}

model quota_settings {
  id                  String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  monthly_token_limit Int      @default(100000)
  daily_token_limit   Int?
  hourly_token_limit  Int?
  cost_limit_usd      Decimal? @db.Decimal(10, 6)
  cost_limit_brl      Decimal? @db.Decimal(10, 6)
  is_active           Boolean  @default(true)
  created_at          DateTime @default(now()) @db.Timestamp(6)
  updated_at          DateTime @default(now()) @db.Timestamp(6)
  role                role     @unique

  @@index([role], map: "idx_quota_settings_role")
}

model enem_session {
  session_id String              @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user_id    String              @db.Uuid
  mode       enem_mode
  area       String[]            @db.VarChar(50)
  config     Json
  status     enem_session_status @default(ACTIVE)
  start_time DateTime            @default(now()) @db.Timestamp(6)
  end_time   DateTime?           @db.Timestamp(6)
  created_at DateTime            @default(now()) @db.Timestamp(6)
  updated_at DateTime            @default(now()) @db.Timestamp(6)
  responses  enem_response[]
  score      enem_score?

  @@index([user_id], map: "idx_enem_sessions_user")
  @@index([area], map: "idx_enem_sessions_area")
  @@index([status], map: "idx_enem_sessions_status")
}

model enem_response {
  response_id     String       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  session_id      String       @db.Uuid
  item_id         String       @db.VarChar(50)
  selected_answer String       @db.VarChar(1)
  time_spent      Int
  is_correct      Boolean
  timestamp       DateTime     @default(now()) @db.Timestamp(6)
  session         enem_session @relation(fields: [session_id], references: [session_id], onDelete: Cascade)

  @@index([session_id], map: "idx_enem_responses_session")
  @@index([item_id], map: "idx_enem_responses_item")
}

model enem_score {
  score_id      String       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  session_id    String       @unique @db.Uuid
  area_scores   Json
  total_score   Float
  tri_estimated Json
  stats         Json
  created_at    DateTime     @default(now()) @db.Timestamp(6)
  session       enem_session @relation(fields: [session_id], references: [session_id], onDelete: Cascade)

  @@index([session_id], map: "idx_enem_scores_session")
}

model lesson_progress {
  id               String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user_id          String    @db.Uuid
  lesson_id        String    @db.VarChar(50)
  created_at       DateTime  @default(now()) @db.Timestamp(6)
  updated_at       DateTime  @default(now()) @db.Timestamp(6)
  completed_stages Int[]     @default([])
  completion_date  DateTime? @db.Timestamp(6)
  current_stage    Int       @default(0)
  is_completed     Boolean   @default(false)
  quiz_results     Json      @default("{}")
  total_time_spent Int       @default(0)

  @@unique([user_id, lesson_id], map: "idx_lesson_progress_user_lesson")
  @@index([user_id], map: "idx_lesson_progress_user")
  @@index([lesson_id], map: "idx_lesson_progress_lesson")
  @@index([is_completed], map: "idx_lesson_progress_completed")
}

model lesson_certificates {
  id               String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user_id          String   @db.Uuid
  lesson_id        String   @db.VarChar(50)
  lesson_title     String   @db.VarChar(200)
  completion_date  DateTime @default(now()) @db.Timestamp(6)
  duration_minutes Int
  total_questions  Int
  correct_answers  Int
  accuracy_percent Float
  quiz_results     Json
  certificate_data Json
  unique_id        String   @unique @db.VarChar(50)
  created_at       DateTime @default(now()) @db.Timestamp(6)
  updated_at       DateTime @default(now()) @db.Timestamp(6)

  @@index([user_id], map: "idx_lesson_certificates_user")
  @@index([lesson_id], map: "idx_lesson_certificates_lesson")
  @@index([completion_date], map: "idx_lesson_certificates_date")
}

model user_achievements {
  id             String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user_id        String   @db.Uuid
